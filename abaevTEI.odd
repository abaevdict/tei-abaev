                <elementSpec ident="etym" mode="change">
                    <content><!-- replicates standard TEI P5 but adds example -->
                        <alternate minOccurs="0" maxOccurs="unbounded">
                            <textNode/>
                            <classRef key="model.gLike"/>
                            <classRef key="model.global"/>
